{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "other-presets",
      "mobx"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
}